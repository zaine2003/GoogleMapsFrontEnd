{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "max-classes-per-file": [2],
    "member-access": [
      false
    ],
    "no-consecutive-blank-lines": [true, 2],
    "no-var-requires": false,
    "no-namespace": [
      false
    ],
    "no-internal-module": false,
    "array-type": [
      "generic"
    ],
    "max-line-length": [
      200
    ],
    "member-ordering": [
      false
    ],
    "no-console": [
      false
    ]
  }

}